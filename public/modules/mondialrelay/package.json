{
  "name": "mondialrelay",
  "version": "3.0.0",
  "description": "Mondial Realay v3",
  "directories": {
    "doc": "202/doc"
  },
  "scripts": {
    "watch": "webpack --mode development --colors --devtool source-map --hide-modules -w",
    "build": "webpack --mode production --colors --devtool source-map --progress -p",
    "doc-build": "stylemark -i 202/doc -o 202/build/doc -c 202/doc/config.yml",
    "doc-watch": "stylemark -i 202/doc -o 202/build/doc -c 202/doc/config.yml -w -b"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.202-ecommerce.com:module-partners/mondialrelay.git"
  },
  "keywords": [
    "mondialrelay"
  ],
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "autoprefixer": "^9.5.1",
    "babel-loader": "^8.0.6",
    "css-loader": "^2.1.1",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylemark": "^3.0.3",
    "terser-webpack-plugin": "^1.3.0",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-fix-style-only-entries": "^0.2.2"
  }
}
